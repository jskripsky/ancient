%%HP: T(3)A(D)F(.);
\<<
"QUADRATISCHE
GLEICHUNG 

INPUT: A B C 

OUTPUT: X1 X2 (D)"
DISPHELP \-> A B C
  \<< 'B^2-4*A*C'
EVAL \-> D
    \<<
      IF 'D<0'
      THEN CLLCD
"KEINE LOESUNG" 1
DISP 1 FREEZE 0
WAIT DROP KILL
      ELSE '-B+\v/D'
EVAL '2*A' EVAL / \->
X1
        \<< '-B-\v/D'
EVAL '2*A' EVAL / \->
X2
          \<< X1 DTAG
'X1' \->TAG X2 DTAG
'X2' \->TAG
          \>>
        \>>
      END
    \>>
  \>>
\>>
